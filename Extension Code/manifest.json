{
    "name" : "MeetMate",
    "version" : "1.0.0",
    "description": "An easy to use browser extension for your everyday online meeting needs.",

    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "meetmate.png"
        
    },

    "icons": {"128": "meetmate128.png"},

    "manifest_version" : 2,

    "content_scripts" :[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["script.js"]
        }
    ]
}